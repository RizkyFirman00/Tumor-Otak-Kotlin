<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Lapisan untuk bentuk dasar -->
    <item>
        <shape android:shape="oval">
            <!-- Atur warna latar belakang sesuai kebutuhan -->
            <solid android:color="@color/young_blue" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>

    <!-- Lapisan untuk gambar -->
    <item
        android:width="30dp"
        android:height="30dp"
        android:bottom="4dp"
        android:drawable="@drawable/baseline_flip_camera_android_24"
        android:gravity="center"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp" />

</layer-list>